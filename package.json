{
  "name": "@osjs/example-react-application",
  "version": "1.1.11",
  "description": "OS.js Example React Application",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint *.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "stylelint": "stylelint index.scss src/**/*.scss",
    "prepublishOnly": "npm run eslint && npm run stylelint && rm ./dist/* && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/os-js/osjs-example-react-application.git"
  },
  "keywords": [
    "osjs"
  ],
  "files": [
    "dist/",
    "server.js",
    "metadata.json"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "http": "0.0.1-security",
    "https": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "stream": "0.0.2",
    "url-loader": "^4.1.1",
    "xmldom": "^0.6.0",
    "xpath": "0.0.32"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "@osjs/dev-meta": "^1.0.3"
  },
  "author": "Anders Evenrud <andersevenrud@gmail.com>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/os-js/osjs-example-react-application/issues"
  },
  "homepage": "https://github.com/os-js/osjs-example-react-application#readme",
  "osjs": {
    "type": "package"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@osjs/eslint-config"
  },
  "stylelint": {
    "extends": "@osjs/stylelint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
